{
  "name": "ModelOrchestrator",
  "version": "1.0.0",
  "description": "Comprehensive model management system providing CLI and web UI for HuggingFace model operations, filesystem normalization, and LLM service integration",
  "intent": "model_orchestrator",
  "author": "Kari Team",
  "license": "MPL-2.0",
  "plugin_type": "ai",
  "provider_id": "model_orchestrator",
  "type": "model_management",
  "enable_external_workflow": true,
  "trusted_ui": true,
  "required_roles": [
    "admin",
    "model_manager"
  ],
  "capabilities": {
    "memory": {
      "read": true,
      "write": true
    },
    "filesystem": {
      "read": true,
      "write": true
    },
    "network": {
      "external_api": true,
      "huggingface_hub": true
    },
    "cli": {
      "commands": [
        "list",
        "info", 
        "download",
        "browse",
        "migrate",
        "ensure",
        "gc"
      ]
    },
    "api": {
      "endpoints": [
        "/api/models/list",
        "/api/models/info",
        "/api/models/download",
        "/api/models/migrate",
        "/api/models/ensure",
        "/api/models/gc",
        "/api/models/registry",
        "/api/models/health",
        "/api/models/compatibility"
      ]
    }
  },
  "config": {
    "models_root": "./models",
    "registry_path": "./models/llm_registry.json",
    "max_concurrent_downloads": 3,
    "enable_metrics": true,
    "enable_rbac": true,
    "offline_mode": false,
    "mirror_url": null,
    "max_storage_gb": null,
    "enable_license_tracking": true,
    "enable_garbage_collection": true,
    "enable_compatibility_checks": true
  },
  "dependencies": {
    "plugins": [
      "ai/llm-services"
    ],
    "services": [
      "auth",
      "metrics", 
      "logging",
      "rbac"
    ],
    "integrations": [
      "llm_registry",
      "llm_settings",
      "prometheus",
      "websocket"
    ]
  },
  "module": "plugin_marketplace.ai.model-orchestrator.service",
  "entry_point": "service:ModelOrchestratorService",
  "runtimes": [
    "python3.10+",
    "huggingface_hub>=0.25.2",
    "rich>=13.7.1",
    "humanfriendly>=10.0",
    "pydantic>=2.0"
  ],
  "tags": [
    "model-management",
    "huggingface",
    "cli",
    "web-ui",
    "filesystem",
    "registry",
    "production"
  ]
}