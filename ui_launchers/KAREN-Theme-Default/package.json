{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/preflight-check.cjs && next dev -p 8000",
    "dev:8010": "node scripts/preflight-check.cjs && next dev -p 8010",
    "dev:safe": "node dev-server.js",
    "dev:turbo": "node scripts/preflight-check.cjs && next dev --turbopack -p 8000",
    "dev:webpack": "node scripts/preflight-check.cjs && next dev -p 8000",
    "analyze": "ANALYZE=true next build",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:tree-shaking": "node scripts/analyze-tree-shaking.js",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:fonts": "echo 'Font optimization integrated in build process'",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "build:fast": "node scripts/fast-build.js",
    "build:production": "npm run optimize:pre-build && next build && npm run optimize:post-build",
    "build:analyze": "npm run analyze && npm run analyze:bundle",
    "prestart": "next build",
    "start": "next start -p 8000",
    "lint": "next lint",
    "lint:accessibility": "eslint --config .eslintrc.accessibility.json \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:accessibility": "vitest run src/__tests__/accessibility/",
    "test:accessibility:watch": "vitest src/__tests__/accessibility/",
    "test:accessibility:coverage": "vitest --coverage src/__tests__/accessibility/",
    "test:accessibility:comprehensive": "vitest run src/__tests__/accessibility/comprehensive-accessibility-suite.test.tsx",
    "test:accessibility:ci": "node scripts/accessibility-ci-integration.js test",
    "test:accessibility:baseline": "node scripts/accessibility-ci-integration.js baseline",
    "test:assistive-tech": "node scripts/assistive-technology-test.js",
    "docs:accessibility": "npx tsx -e \"import('./src/lib/accessibility/documentation-generator.ts').then(m => m.generateAccessibilityDocs())\"",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:admin": "vitest run src/__tests__/integration/admin-*.test.*",
    "test:admin:watch": "vitest src/__tests__/integration/admin-*.test.*",
    "test:admin:coverage": "vitest --coverage src/__tests__/integration/admin-*.test.*",
    "test:admin:integration": "npx tsx src/__tests__/integration/run-admin-integration-tests.ts",
    "test:admin:e2e": "playwright test src/__tests__/e2e/admin-e2e.spec.ts",
    "test:admin:all": "npm run test:admin && npm run test:admin:e2e",
    "test:performance": "vitest run src/lib/performance/__tests__/",
    "test:performance:watch": "vitest src/lib/performance/__tests__/",
    "test:performance:coverage": "vitest --coverage src/lib/performance/__tests__/",
    "test:performance:benchmark": "npx tsx src/scripts/run-performance-tests.ts",
    "storybook": "storybook dev -p 6006",
    "postinstall": "patch-package --error-on-fail",
    "optimize:pre-build": "node scripts/clean-development-artifacts.js && node scripts/optimize-css-js.js",
    "optimize:post-build": "npm run analyze:bundle",
    "analyze:production": "ANALYZE=true npm run build:production",
    "qa:report": "tsx src/scripts/qa-report.ts"
  },
