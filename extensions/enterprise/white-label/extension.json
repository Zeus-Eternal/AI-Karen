{
  "name": "white-label-branding",
  "version": "1.0.0",
  "display_name": "White Label Branding Suite",
  "description": "Enterprise white-label customization with branding, theming, and multi-tenant configuration management",
  "author": "Kari AI Team",
  "license": "Commercial",
  "category": "enterprise",
  "tags": ["white-label", "branding", "theming", "enterprise", "customization"],
  
  "api_version": "1.0",
  "kari_min_version": "0.4.0",
  
  "capabilities": {
    "provides_ui": true,
    "provides_api": true,
    "provides_background_tasks": false,
    "provides_webhooks": false
  },
  
  "dependencies": {
    "plugins": [],
    "extensions": [],
    "system_services": ["postgres"]
  },
  
  "permissions": {
    "data_access": ["read", "write", "admin"],
    "plugin_access": [],
    "system_access": ["files", "themes"],
    "network_access": []
  },
  
  "resources": {
    "max_memory_mb": 128,
    "max_cpu_percent": 10,
    "max_disk_mb": 512
  },
  
  "ui": {
    "control_room_pages": [
      {
        "name": "Branding Center",
        "path": "/branding",
        "icon": "ðŸŽ¨",
        "permissions": ["enterprise", "admin"]
      }
    ],
    "streamlit_pages": [
      {
        "name": "Brand Analytics",
        "module": "ui.brand_analytics",
        "permissions": ["enterprise"]
      }
    ]
  },
  
  "api": {
    "endpoints": [
      {
        "path": "/branding/themes",
        "methods": ["GET", "POST", "PUT"],
        "permissions": ["enterprise"]
      },
      {
        "path": "/branding/assets",
        "methods": ["GET", "POST", "DELETE"],
        "permissions": ["enterprise"]
      },
      {
        "path": "/branding/config",
        "methods": ["GET", "PUT"],
        "permissions": ["enterprise"]
      }
    ]
  },
  
  "marketplace": {
    "price": "enterprise",
    "support_url": "https://github.com/kari-ai/extensions",
    "documentation_url": "https://docs.kari.ai/extensions/white-label-branding",
    "screenshots": ["branding-center.png", "theme-editor.png", "brand-preview.png"]
  }
}