# AI-Karen Optimized Development Environment
# These settings enable aggressive resource optimization

# Lazy loading settings
KARI_LAZY_LOADING=true
KARI_MINIMAL_MODE=true
KARI_DEFER_HEAVY_SERVICES=true
KARI_ESSENTIAL_ONLY=true
KARI_ULTRA_MINIMAL=false
KARI_MAX_STARTUP_TIME=5.0
KARI_RESOURCE_MONITORING=true

# Performance optimization
ENABLE_PERFORMANCE_OPTIMIZATION=true
KARI_FAST_STARTUP=true
FAST_STARTUP=true

# Service timeouts
KARI_SERVICE_IDLE_TIMEOUT=180
KARI_CLEANUP_INTERVAL=60

# Resource limits
KARI_MAX_MEMORY_MB=1024
KARI_MAX_CPU_PERCENT=70

# Development mode
KARI_DEPLOYMENT_MODE=development
ENVIRONMENT=development

# Router optimization
KARI_DEFER_ROUTER_WIRING=true

# GPU optimization
KARI_LIMIT_GPU_MEMORY=true
KARI_GPU_MEMORY_LIMIT_MB=1024

# Disable heavy features in development
KARI_DISABLE_ANALYTICS=false
KARI_DISABLE_TELEMETRY=false
KARI_ENABLE_GRACEFUL_DEGRADATION=true

# Logging optimization
KARI_LOG_LEVEL=INFO
KARI_REDUCE_LOGGING=false

# Model loading optimization
KARI_LAZY_MODEL_LOADING=true
KARI_MODEL_CACHE_SIZE=128
