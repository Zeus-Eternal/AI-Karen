{
  "id": "worker_agent",
  "name": "Worker Agent",
  "type": "worker",
  "version": "1.0.0",
  "description": "A general-purpose worker agent for handling single-domain tasks",
  "entry_point": "handler.py",
  "requirements": [],
  "capabilities": [
    {
      "name": "task_execution",
      "description": "Execute tasks within a single domain",
      "version": "1.0.0",
      "parameters": {
        "type": {
          "type": "string",
          "description": "The type of task to execute",
          "enum": ["data_processing", "analysis"],
          "required": true
        },
        "data": {
          "type": "object",
          "description": "The data to process or analyze",
          "required": true
        }
      }
    }
  ],
  "dependencies": {},
  "environment": {
    "python_version": "3.9+"
  },
  "resources": {
    "cpu": "1",
    "memory": "1GB"
  },
  "permissions": ["read", "execute"],
  "metadata": {
    "author": "Kari Team",
    "created_at": "2025-11-26T00:00:00Z",
